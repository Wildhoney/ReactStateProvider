    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "browser-env": "^3.2.6",
    "concurrently": "^4.1.1",
    "core-js": "^3.1.4",
    "delay": "^4.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
