{
  "name": "react-store-provider",
  "version": "0.1.0",
  "description": "Another way to manage global state in any given component tree using providers and hooks. ",
  "main": "dist/rsp.cjs.js",
  "module": "dist/rsp.esm.js",
  "repository": "git@github.com:Wildhoney/ReactStateProvider.git",
